    "greenlion/php-sql-parser": "4.3.*",
    "mpdf/mpdf": "8.0.*",
    "squizlabs/php_codesniffer": "3.5.*",
    "natxet/cssmin": "3.0.*",
    "sunra/php-simple-html-dom-parser": "1.5.*",
    "leafo/lessphp": "0.5.*",
    "scssphp/scssphp": "1.0.7",
    "tedivm/jshrink": "1.3.*",
    "phpmailer/phpmailer": "6.4.*",
    "tijsverkoyen/css-to-inline-styles": "2.2.*",
    "dusterio/link-preview": "1.2.*",
