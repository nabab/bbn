{
  "name": "bbn/bbn",
  "description": "PHP opinionated framework",
  "keywords": ["database","mvc","appui"],
  "homepage": "https://github.com/nabab/bbn",
  "license": "MIT",
  "authors": [
    {
      "name": "Thomas Nabet <thomas@bbn.so>",
      "email": "thomas.nabet@gmail.com",
      "homepage": "https://bbn.solutions",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.0",
    "ext-mbstring": "*",
    "ext-json": "*",
    "composer/composer": "2.3.5",
    "zordius/lightncandy": "1.2.*",
    "greenlion/php-sql-parser": "4.5.*",
    "mpdf/mpdf": "8.0.*",
    "natxet/cssmin": "3.0.*",
    "sunra/php-simple-html-dom-parser": "1.5.*",
    "scssphp/scssphp": "1.6.*",
    "tedivm/jshrink": "1.4.*",
    "phpmailer/phpmailer": "6.8.*",
    "tijsverkoyen/css-to-inline-styles": "2.2.*",
    "dusterio/link-preview": "1.2.*",
    "wapmorgan/unified-archive": "0.1.*",
    "pear/archive_tar": "1.4.*",
    "gettext/gettext": "4.8.1",
    "nesbot/carbon": "2.57.*",
    "nikic/php-parser": "4.15.*",
    "sabre/dav": "4.1.*",
    "erusev/parsedown": "1.7.*",
    "phpdocumentor/reflection-docblock": "5.2.*",
    "sepia/po-parser": "6.0.*",
    "tplaner/when": "3.0.*",
    "mck89/peast": "1.13.*",
    "czproject/git-php": "3.18.*",
    "firebase/php-jwt": "5.2.*",
    "mollie/oauth2-mollie-php": "^2.0",
    "brick/phonenumber": "0.3.*",
    "opis/closure": "^3.5",
    "ezyang/htmlpurifier": "^4.13",
    "xemlock/htmlpurifier-html5": "dev-master",
    "psr/simple-cache": "1.0.*"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "3.6.*",
    "phpunit/phpunit": "10.1.1",
    "pdepend/pdepend": "2.10.*",
    "phpmd/phpmd": "2.10.*",
    "friendsofphp/php-cs-fixer": "3.3.*",
    "phpunitgen/core": "3.0.*",
    "mockery/mockery": "1.4.*"
  },
  "autoload": {
    "psr-4": {
      "bbn\\": "src/bbn/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "bbn\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable" : true
}
