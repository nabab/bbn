            "final": false,
            "code": "function(SimpleXMLElement $xml = NULL)\n  {\n\t\tif ($xml === null) {\n\t\t\t$xml = $this->xml;\n\t\t}\n\n\t\tif (!$xml->children()) {\n\t\t\treturn (string) $xml;\n\t\t}\n\n\t\t$arr = [];\n\t\tforeach ($xml->children() as $tag => $child) {\n\t\t\tif (count($xml->$tag) === 1) {\n\t\t\t\t$arr[$tag] = $this->toArray($child);\n\t\t\t} else {\n\t\t\t\t$arr[$tag][] = $this->toArray($child);\n\t\t\t}\n\t\t}\n\n\t\treturn $arr;\n  }",
