            "summary": "Adds custom fonts",
            "description": "$pdf->addFonts([\n 'dawningofanewday' => [\n   'R' => BBN_DATA_PATH.'files\/DawningofaNewDay.ttf'\n  ]\n]);",
            "description_parts": [
                {
                    "type": "text",
                    "content": "<p>$pdf-&gt;addFonts([\n'dawningofanewday' =&gt; [\n'R' =&gt; BBN_DATA_PATH.'files\/DawningofaNewDay.ttf'\n]\n]);<\/p>"
